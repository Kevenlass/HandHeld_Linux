            <DropShadow />
         </effect></ComboBox>
      <Label layoutX="169.0" layoutY="27.0" text="Kunde" textFill="#f8f8f8">
         <font>
            <Font name="Times New Roman Bold" size="24.0" />
         </font>
      </Label>
      <Label layoutX="158.0" layoutY="122.0" text="Standort" textFill="#f8f8f8">
         <font>
            <Font name="Times New Roman Bold" size="24.0" />
