                  <Font name="Times New Roman Bold" size="18.0" />
               </font>
            </Button>
            <Button fx:id="new_Device_Button" layoutX="11.0" layoutY="190.0" mnemonicParsing="false" onAction="#New_Dataset_Device" prefHeight="38.0" prefWidth="178.0" text="Neues Gerät">
