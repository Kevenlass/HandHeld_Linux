<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #303030;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Main.controller.Controller_Dataset_Filter">
   <children>
      <ComboBox fx:id="comboFieldKunde" layoutX="28.0" layoutY="55.0" onAction="#comboAction_Kunde" prefHeight="38.0" prefWidth="351.0">
