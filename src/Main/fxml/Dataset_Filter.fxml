            <DropShadow />
         </effect></ComboBox>
      <Pane layoutX="400.0" prefHeight="400.0" prefWidth="200.0" style="-fx-background-color: black;">
         <children>
            <Button fx:id="get_Dataset_Database_Button" layoutX="11.0" layoutY="349.0" mnemonicParsing="false" onAction="#filteredWindow" prefHeight="38.0" prefWidth="178.0" text="Daten Anfordern">
